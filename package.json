{
  "name": "react-easy-styleguide",
  "version": "2.0.1",
  "description": "Easy styleguide/component library tool for React projects with support for global styles. Works great with create-react-app",
  "main": "dist/react-easy-styleguide.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "release": "standard-version",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "styleguide",
    "component-library"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "Marco Hamersma",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^0.26.1",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.5.1",
    "lodash.isplainobject": "^4.0.6",
    "lodash.map": "^4.6.0",
    "marked": "^0.3.6",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "react": "^15.4.2",
    "react-bem-helper": "^1.2.0",
    "sass-loader": "^8.0.0",
    "slug": "^0.9.1",
    "standard-version": "^7.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcohamersma/react-easy-styleguide.git"
  },
  "bugs": {
    "url": "https://github.com/marcohamersma/react-easy-styleguide/issues"
  },
  "homepage": "https://github.com/marcohamersma/react-easy-styleguide#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
