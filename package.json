{
  "name": "react-easy-styleguide",
  "version": "2.1.0",
  "description": "Easy styleguide/component library tool for React projects with support for global styles. Works great with create-react-app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist/* && webpack --mode=production",
    "dev": "webpack --watch",
    "release": "standard-version",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "styleguide",
    "component-library"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "Marco Hamersma",
  "license": "MIT",
  "peerDependencies": {
    "react": ">16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-react-perf": "^3.2.1",
    "husky": "^3.0.5",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "react": "^15.4.2",
    "react-bem-helper": "^1.2.0",
    "sass": "^1.80.6",
    "sass-loader": "^10.5.2",
    "slug": "^0.9.4",
    "standard-version": "^7.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcohamersma/react-easy-styleguide.git"
  },
  "bugs": {
    "url": "https://github.com/marcohamersma/react-easy-styleguide/issues"
  },
  "homepage": "https://github.com/marcohamersma/react-easy-styleguide#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
